{
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "configPath": "",
    "disableLanguages": [],
    "documentSelectors": [],
    "embeddedLanguageFormatting": "auto",
    "enable": true,
    "enableDebugLogs": false,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "ignorePath": ".prettierignore",
    "importOrder": [
        "^vue$", 
        "<TYPES>",
        "<TYPES>^[.]",
        "<BUILTIN_MODULES>",
        "<THIRD_PARTY_MODULES>",
        "",
        "^@/component-library/(.*)$", 
        "",
        "^@/domain/(.*)$", 
        "",
        "^@/primary/(.*)$", 
        "",
        "^@/secondary/(.*)$", 
        "",
        "^@/utils/(.*)$",
        "",
        "^[./]"
    ],
    "insertPragma": false,
    "jsxSingleQuote": false,
    "packageManager": "npm",
    "prettierPath": "",
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requireConfig": false,
    "requirePragma": false,
    "resolveGlobalModules": false,
    "semi": true,
    "singleAttributePerLine": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "none",
    "useEditorConfig": true,
    "useTabs": false,
    "vueIndentScriptAndStyle": false,
    "withNodeModules": false
}
